{
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "final": "china",
    "independent_cache": true,
    "servers": [
      {
        "tag": "china",
        "type": "local",
        "prefer_go": true
      },
      {
        "tag": "fake",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      },
      {
        "tag": "ts",
        "type": "tailscale",
        "endpoint": "ts"
      }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [{ "domain_suffix": ["push.apple.com"] }],
        "server": "china"
      },
      { "domain_suffix": ["alai-wezen.ts.net"], "server": "ts" },
      {
        "type": "logical",
        "mode": "and",
        "rules": [{ "query_type": "HTTPS" }, { "rule_set": "global" }],
        "action": "predefined",
        "rcode": "NOERROR"
      },
      { "query_type": ["A", "AAAA"], "rewrite_ttl": 30, "server": "fake" }
    ]
  },
  "inbounds": [
    {
      "tag": "http",
      "type": "http",
      "listen": "127.0.0.1",
      "listen_port": 6152
    },
    {
      "tag": "socks",
      "type": "socks",
      "listen": "127.0.0.1",
      "listen_port": 6153
    },
    {
      "tag": "tun",
      "type": "tun",
      "address": ["172.18.0.1/30", "fdfe:dcba:9876::1/126"],
      "auto_route": true,
      "loopback_address": ["10.7.0.1"],
      "stack": "mixed",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 6152,
          "bypass_domain": ["*.local", "*.push.apple.com"]
        }
      },
      "route_exclude_address": [
        "17.0.0.0/8",
        "2403:300::/32",
        "2620:149::/32",
        "2a01:b740::/32"
      ]
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": ["x1", "x2"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "final",
      "type": "selector",
      "outbounds": ["x2", "x1", "direct"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "low",
      "type": "selector",
      "outbounds": ["x2", "x1", "direct"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "x1",
      "type": "urltest",
      "outbounds": [],
      "tolerance": 80,
      "interrupt_exist_connections": true
    },
    {
      "tag": "x2",
      "type": "urltest",
      "outbounds": [],
      "tolerance": 80,
      "interrupt_exist_connections": true
    },
    {
      "tag": "hk",
      "type": "urltest",
      "outbounds": [],
      "url": "http://latency-test.skk.moe/endpoint"
    },
    {
      "tag": "us",
      "type": "urltest",
      "outbounds": [],
      "url": "http://latency-test.skk.moe/endpoint"
    },
    {
      "tag": "direct",
      "type": "direct"
    }
  ],
  "route": {
    "default_domain_resolver": "china",
    "auto_detect_interface": true,
    "final": "final",
    "rules": [
      { "inbound": "tun", "action": "sniff" },
      { "protocol": "dns", "action": "hijack-dns" },
      { "ip_cidr": ["100.64.0.0/10"], "outbound": "ts" },
      { "ip_is_private": true, "outbound": "direct" },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          { "protocol": "quic" },
          { "rule_set": ["global", "ai", "imgur", "paypal"] }
        ],
        "action": "reject"
      },
      { "rule_set": ["ai", "paypal"], "outbound": "us" },
      { "rule_set": ["imgur"], "outbound": "hk" },
      {
        "rule_set": ["wechat", "apple", "chinamax", "direct"],
        "outbound": "direct"
      },
      { "rule_set": ["global", "tg"], "outbound": "proxy" },
      { "action": "resolve", "server": "china" },
      { "rule_set": "chinaip", "outbound": "direct" }
    ],
    "rule_set": [
      {
        "tag": "direct",
        "download_detour": "direct",
        "type": "remote",
        "url": "https://static.0x8.site/sing-box/rule/direct.json"
      },
      {
        "tag": "chinamax",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/ChinaMaxNoIP/ChinaMaxNoIP.srs"
      },
      {
        "tag": "global",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/Global/Global.srs"
      },
      {
        "tag": "ai",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://github.com/SagerNet/sing-geosite/raw/refs/heads/rule-set/geosite-category-ai-!cn.srs"
      },
      {
        "tag": "imgur",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/Imgur/Imgur.srs"
      },
      {
        "tag": "paypal",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/PayPal/PayPal.srs"
      },
      {
        "tag": "tg",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/Telegram/Telegram.srs"
      },
      {
        "tag": "wechat",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/WeChat/WeChat.srs"
      },
      {
        "tag": "apple",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/Apple/Apple.srs"
      },
      {
        "tag": "chinaip",
        "download_detour": "proxy",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/quanuanc/singbox_ruleset@main/rule/ChinaIPs/ChinaIPs.srs"
      }
    ]
  },
  "endpoints": [
    {
      "tag": "ts",
      "type": "tailscale",
      "hostname": "{ts_hostname}",
      "auth_key": "{ts_auth_key}"
    }
  ],
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "[::]:9090",
      "secret": "1118",
      "access_control_allow_origin": ["*"],
      "access_control_allow_private_network": true
    }
  }
}
