        "outbound": "direct"
      },
      {
        "rule_set": ["speedtest", "cdn", "stream", "download"],
        "outbound": "low"
      },
      { "rule_set": ["tg", "myproxy", "global"], "outbound": "proxy" },
      { "action": "resolve", "server": "china" },
      { "rule_set": "cn_ip", "outbound": "direct" }
