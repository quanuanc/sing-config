{
  "log": {
    "disabled": false,
    "level": "fatal",
    "timestamp": true
  },
  "dns": {
    "final": "china",
    "independent_cache": true,
    "servers": [
      {
        "tag": "china",
        "server": "221.5.88.88",
        "type": "udp"
      },
      {
        "tag": "fake",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      },
      {
        "tag": "ts",
        "type": "tailscale",
        "endpoint": "ts"
      }
    ],
    "rules": [
      { "rule_set": "rejectdrop", "action": "reject", "method": "default" },
      { "rule_set": "reject", "action": "reject", "method": "default" },
      { "domain": ["captive.apple.com"], "server": "china" },
      { "domain_suffix": ["alai-wezen.ts.net"], "server": "ts" },
      {
        "type": "logical",
        "mode": "and",
        "rules": [{ "query_type": "HTTPS" }, { "rule_set": "global" }],
        "action": "predefined",
        "rcode": "NOERROR"
      },
      { "query_type": ["A", "AAAA"], "rewrite_ttl": 30, "server": "fake" }
    ]
  },
  "inbounds": [
    {
      "tag": "http",
      "type": "http",
      "listen": "127.0.0.1",
      "listen_port": 6152
    },
    {
      "tag": "socks",
      "type": "socks",
      "listen": "127.0.0.1",
      "listen_port": 6153
    },
    {
      "tag": "tun",
      "type": "tun",
      "address": ["172.18.0.1/30", "fdfe:dcba:9876::1/126"],
      "auto_route": true,
      "loopback_address": ["10.7.0.1"],
      "stack": "mixed",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 6152,
          "bypass_domain": ["*.local", "captive.apple.com"]
        }
      }
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": ["x1", "x2"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "final",
      "type": "selector",
      "outbounds": ["x2", "x1", "direct"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "low",
      "type": "selector",
      "outbounds": ["x2", "x1", "direct"],
      "interrupt_exist_connections": true
    },
    {
      "tag": "x1",
      "type": "urltest",
      "outbounds": [],
      "tolerance": 80,
      "interrupt_exist_connections": true
    },
    {
      "tag": "x2",
      "type": "urltest",
      "outbounds": [],
      "tolerance": 80,
      "interrupt_exist_connections": true
    },
    {
      "tag": "hk",
      "type": "urltest",
      "outbounds": [],
      "url": "http://latency-test.skk.moe/endpoint"
    },
    {
      "tag": "us",
      "type": "urltest",
      "outbounds": [],
      "url": "http://latency-test.skk.moe/endpoint"
    },
    {
      "tag": "direct",
      "type": "direct"
    }
  ],
  "route": {
    "default_domain_resolver": "china",
    "auto_detect_interface": true,
    "final": "final",
    "rules": [
      {
        "inbound": "tun",
        "action": "sniff",
        "sniffer": ["http", "tls", "quic", "dns"]
      },
      { "protocol": "dns", "action": "hijack-dns" },
      {"rule_set": "rejectdrop", "action": "reject", "method": "default" },
      {"rule_set": "reject", "action": "reject", "method": "default" },
      { "ip_cidr": ["100.64.0.0/10"], "outbound": "ts" },
      { "ip_is_private": true, "outbound": "direct" },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          { "protocol": "quic" },
          {
            "rule_set": [
              "myai",
              "ai",
              "paypal",
              "speedtest",
              "cdn",
              "stream",
              "download",
              "myproxy",
              "global"
            ]
          }
        ],
        "action": "reject"
      },
      { "rule_set": ["myai", "ai", "paypal"], "outbound": "us" },
      {
        "rule_set": [
          "mydirect",
          "wechat",
          "apple_cdn",
          "apple_service",
          "apple_cn",
          "neteasemusic",
          "domestic",
          "skkdirect",
          "microsoft_cdn",
          "microsoft"
